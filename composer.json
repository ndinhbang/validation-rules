{
    "name": "ndinhbang/validation-rules",
    "description": "A laravel package to provides additional validation rules",
    "keywords": [
        "ndinhbang",
        "laravel",
        "validatation",
        "rules",
        "validation-rules"
    ],
    "homepage": "https://github.com/ndinhbang/validation-rules",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dinh Bang Nguyen",
            "email": "ndinhbang111@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "ndinhbang/support": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Ndinhbang\\ValidationRules\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ndinhbang\\ValidationRules\\ValidationRulesServiceProvider"
            ]
        }
    }
}
